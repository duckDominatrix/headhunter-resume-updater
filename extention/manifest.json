{
  "manifest_version": 2,
  "name": "update resume",
  "description": "Extention that's help you update your resume on the hh.ru resume page",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://hh.ru/*",
    "https://hh.ru/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}