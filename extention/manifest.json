{
  "manifest_version": 2,
  "name": "headhunter resume updater",
  "description": "Updates your resume on the hh.ru resume page",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://hh.ru/*",
    "https://hh.ru/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}